                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                            </div>
                            @*lkj*@
                            <!--/navbar header-->
                            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                                <ul class="nav navbar-nav">
                                    <li class="active"><a href="index.html">Home</a></li>
                                    <li><a href="about.html">Champions</a></li>
                                    <li class="dropdown">
                                        <a href="#" class="scroll dropdown-toggle" data-toggle="dropdown">Leaderboard<b class="caret"></b></a>
                                        <ul class="dropdown-menu">
                                            <li><a href="review.html">Review 1</a></li>
                                            <li><a href="review.html">Review 2</a></li>
                                            <li><a href="review.html">Review 3</a></li>
                                        </ul>
                                    </li>
                                    <li><a href="gallery.html">Stream</a></li>
                                    <li><a href="shortcodes.html">About</a></li>
                                    <li><a href="contact.html">Contact</a></li>
                                </ul>
                            </div>
                            <!--/navbar collapse-->
